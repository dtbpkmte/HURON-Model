            <mesh filename="package://huron_description/meshes/l_ankle_roll_link.STL" scale="1 1 1" />
        </geometry>
        <material name="">
            <color rgba="0.77647 0.75686 0.73725 1" />
        </material>
    </xacro:macro>
    

    <!-- right hip yaw link -->
    <xacro:macro name="insert_mesh_r_hip_yaw_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
            <mesh filename="package://huron_description/meshes/r_hip_yaw_link.STL" scale="1 1 1" />
        </geometry>
        <material name="grey"/>
    </xacro:macro>
    <!-- right hip roll link -->
    <xacro:macro name="insert_mesh_r_hip_roll_link">
