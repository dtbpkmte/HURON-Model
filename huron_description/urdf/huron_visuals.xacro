<?xml version="1.0"?>
<robot 
    xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!-- body link -->
    <xacro:macro name="insert_mesh_base_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
            <mesh filename="package://huron_description/meshes/base_link.STL" scale="1 1 1" />
        </geometry>
        <material name="grey"/>
    </xacro:macro>

    <!-- Battery -->
    <xacro:macro name="insert_mesh_battery_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
            <box size="0.065 0.188 0.151" />
        </geometry>
        <material name="blue"/>
    </xacro:macro>

    <!-- left hip yaw link -->
    <xacro:macro name="insert_mesh_l_hip_yaw_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
            <mesh filename="package://huron_description/meshes/l_hip_yaw_link.STL" scale="1 1 1" />
        </geometry>
        <material name="grey"/>
    </xacro:macro>
    <!-- left hip roll link -->
    <xacro:macro name="insert_mesh_l_hip_roll_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
            <mesh filename="package://huron_description/meshes/l_hip_roll_link.STL" scale="1 1 1" />
        </geometry>
        <material name="grey"/>
    </xacro:macro>
    <!-- left hip pitch link -->
    <xacro:macro name="insert_mesh_l_hip_pitch_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
        <geometry>
            <mesh filename="package://huron_description/meshes/l_hip_pitch_link.STL" scale="1 1 1" />
        </geometry>
        <material name="grey"/>
    </xacro:macro>
    <!-- left knee pitch link -->
    <xacro:macro name="insert_mesh_l_knee_pitch_link">
        <origin xyz="0 0 0" rpy="0 0 0" />
