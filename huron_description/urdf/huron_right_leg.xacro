<?xml version="1.0"?>
<robot 
    xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!-- right leg -->
    <xacro:macro name="huron_right_leg" params="parent dxl_effort dxl_speed">
        <!-- right hip yaw joint -->
        <joint
          name="r_hip_yaw_joint"
          type="revolute">
          <origin
            xyz="0.0774999999999944 0 -0.1451242"
            rpy="0 0 3.13046853298182" />
          <parent
            link="${parent}" />
          <child
            link="r_hip_yaw_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="100" />
        </joint>
        <xacro:insert_transmission name="r_hip_yaw_joint"/>
        <!-- right hip yaw link -->
        <link name="r_hip_yaw_link">
            <visual>
                <xacro:insert_mesh_r_hip_yaw_link />
            </visual>
            <collision>
                <xacro:insert_mesh_r_hip_yaw_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_r_hip_yaw_link />
            </inertial>
        </link>
        <!-- right hip roll joint-->
        <joint
          name="r_hip_roll_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="1.5708 0 0" />
          <parent
            link="r_hip_yaw_link" />
          <child
            link="r_hip_roll_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="100" />
        </joint>
        <xacro:insert_transmission name="r_hip_roll_joint"/>
        <!-- right hip roll link -->
        <link name="r_hip_roll_link">
            <visual>
                <xacro:insert_mesh_r_hip_roll_link />
            </visual>
            <collision>
                <xacro:insert_mesh_r_hip_roll_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_r_hip_roll_link />
            </inertial>
        </link>
        <!-- right hip pitch joint -->
        <joint
          name="r_hip_pitch_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="-1.5708 0 -1.5708" />
          <parent
            link="r_hip_roll_link" />
          <child
            link="r_hip_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="100" />
        </joint>
        <xacro:insert_transmission name="r_hip_pitch_joint"/>
        <!-- right hip pitch link -->
        <link name="r_hip_pitch_link">
            <visual>
                <xacro:insert_mesh_r_hip_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_r_hip_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_r_hip_pitch_link />
            </inertial>
        </link>
        <!-- right knee pitch joint -->
        <joint
          name="r_knee_pitch_joint"
          type="revolute">
          <origin
            xyz="0.512778478308813 4.99999999867273E-05 -1.66647320091612E-05"
            rpy="0 0 0" />
          <parent
            link="r_hip_pitch_link" />
          <child
            link="r_knee_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="100" />
        </joint>
        <xacro:insert_transmission name="r_knee_pitch_joint"/>
        <!-- right knee pitch link -->
        <link name="r_knee_pitch_link">
            <visual>
                <xacro:insert_mesh_r_knee_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_r_knee_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_r_knee_pitch_link />
            </inertial>
        </link>
        <!-- right ankle pitch joint -->
        <joint
          name="r_ankle_pitch_joint"
          type="revolute">
          <origin
            xyz="0.370000000000006 -3.16358749707368E-05 -0.00249999999999918"
            rpy="0 0 0.0095311367408488" />
          <parent
            link="r_knee_pitch_link" />
          <child
            link="r_ankle_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="100" />
        </joint>
        <xacro:insert_transmission name="r_ankle_pitch_joint"/>
        <!-- right ankle pitch link -->
        <link name="r_ankle_pitch_link">
            <visual>
                <xacro:insert_mesh_r_ankle_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_r_ankle_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_r_ankle_pitch_link />
            </inertial>
        </link>
        <!-- right ankle roll joint -->
        <joint
          name="r_ankle_roll_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="1.57079632679489 0 0" />
          <parent
            link="r_ankle_pitch_link" />
          <child
            link="r_ankle_roll_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="100" />
        </joint>
        <xacro:insert_transmission name="r_ankle_roll_joint"/>
        <!-- right ankle roll link -->
        <link name="r_ankle_roll_link">
            <visual>
                <xacro:insert_mesh_r_ankle_roll_link />
            </visual>
            <collision>
                <xacro:insert_mesh_r_ankle_roll_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_r_ankle_roll_link />
            </inertial>
        </link>
        
        <!-- Right foot sensors -->
        <!-- right ft fixed joint 1 -->
        <joint name="r1_ft_fixed" type="revolute">
            <origin
              xyz="0.098271 0.014348 0.12788"
              rpy="-0.0095317 0.011124 1.5707" />
            <axis xyz="0 0 1"/>
            <parent link="r_ankle_roll_link"/>
            <child link="r1_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- right foot ft link 1 -->
        <link name="r1_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_r1_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_r1_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_r1_foot_ft_link />
	    </inertial>
        </link>
        

        <!-- right ft fixed joint 2 -->
        <joint name="r2_ft_fixed" type="revolute">
            <origin
              xyz="0.098334 -0.020652 0.12788"
              rpy="-0.0095317 0.011124 1.5707" />
            <axis xyz="0 0 1"/>
            <parent link="r_ankle_roll_link"/>
            <child link="r2_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- right foot ft link 2 -->
        <link name="r2_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_r2_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_r2_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_r2_foot_ft_link />
	    </inertial>
        </link>



        <!-- right ft fixed joint 3 -->
        <joint name="r3_ft_fixed" type="revolute">
            <origin
              xyz="0.098328 -0.01683 0"
              rpy="-0.0095317 0.011124 1.5707" />
            <axis xyz="0 0 1"/>
            <parent link="r_ankle_roll_link"/>
            <child link="r3_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- right foot ft link 3 -->
        <link name="r3_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_r3_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_r3_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_r3_foot_ft_link />
	    </inertial>
        </link>



        <!-- right ft fixed joint 4 -->
        <joint name="r4_ft_fixed" type="revolute">
            <origin
              xyz="0.098267 0.016679 0"
              rpy="-0.0095317 0.011124 1.5707" />
            <axis xyz="0 0 1"/>
            <parent link="r_ankle_roll_link"/>
            <child link="r4_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- right4 foot ft link 4 -->
        <link name="r4_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_r4_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_r4_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_r4_foot_ft_link />
	    </inertial>
        </link>

        <!-- right ft fixed joint -->
        <joint name="rv_ft_fixed" type="fixed">
            <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
            <axis xyz="0 0 0"/>
            <parent link="r4_foot_ft_link"/>
            <child link="r_foot_v_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- right foot ft link -->
        
        <link name="r_foot_v_ft_link"></link>

    </xacro:macro>

</robot>
