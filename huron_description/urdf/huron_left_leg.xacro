<?xml version="1.0"?>
<robot 
    xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!-- left leg -->
    <xacro:macro name="huron_left_leg" params="parent dxl_effort dxl_speed">
        <!-- left hip yaw joint -->
        <joint name="l_hip_yaw_joint" type="revolute">
          <origin
            xyz="-0.0775000000000011 0 -0.1451242"
            rpy="0 0 0" />
          <parent link="${parent}" />
          <child link="l_hip_yaw_link" />
          <axis xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="1" />
        </joint>
        <xacro:insert_transmission name="l_hip_yaw_joint"/>
        <!-- left hip yaw link -->
        <link name="l_hip_yaw_link">
            <visual>
                <xacro:insert_mesh_l_hip_yaw_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_hip_yaw_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_hip_yaw_link />
            </inertial>
        </link>
        <!-- left hip roll joint-->
        <joint
          name="l_hip_roll_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="${pi/2} 0 ${pi}" />
          <parent
            link="l_hip_yaw_link" />
          <child
            link="l_hip_roll_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-0.01"
            upper="0.5236"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="1" />
        </joint>
        <xacro:insert_transmission name="l_hip_roll_joint"/>
        <!-- left hip roll link -->
        <link name="l_hip_roll_link">
            <visual>
                <xacro:insert_mesh_l_hip_roll_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_hip_roll_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_hip_roll_link />
            </inertial>
        </link>
        <!-- left hip pitch joint -->
        <joint
          name="l_hip_pitch_joint"
          type="revolute">
          <origin
            xyz="1.6665E-05 -0.0165 -5E-05"
            rpy="${-pi/2} 0 ${-pi/2}" />
          <parent
            link="l_hip_roll_link" />
          <child
            link="l_hip_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-1.2217"
            upper="1.2217"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="1" />
        </joint>
        <xacro:insert_transmission name="l_hip_pitch_joint"/>
        <!-- left hip pitch link -->
        <link name="l_hip_pitch_link">
            <visual>
                <xacro:insert_mesh_l_hip_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_hip_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_hip_pitch_link />
            </inertial>
        </link>
        <!-- left knee pitch joint -->
        <joint
          name="l_knee_pitch_joint"
          type="revolute">
          <origin
            xyz="0.49628 0 0"
            rpy="0 0 0" />
          <parent
            link="l_hip_pitch_link" />
          <child
            link="l_knee_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="${-pi/2}"
            upper="${pi/2}"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="1" />
        </joint>
        <xacro:insert_transmission name="l_knee_pitch_joint"/>
        <!-- left knee pitch link -->
        <link name="l_knee_pitch_link">
            <visual>
                <xacro:insert_mesh_l_knee_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_knee_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_knee_pitch_link />
            </inertial>
        </link>
        <!-- left ankle pitch joint -->
        <joint
          name="l_ankle_pitch_joint"
          type="revolute">
          <origin
            xyz="0.37 0 0"
            rpy="0 0 0.0" />
          <parent
            link="l_knee_pitch_link" />
          <child
            link="l_ankle_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-0.5236"
            upper="0.1745"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="1" />
        </joint>
        <xacro:insert_transmission name="l_ankle_pitch_joint"/>
        <!-- left ankle pitch link -->
        <link name="l_ankle_pitch_link">
            <visual>
                <xacro:insert_mesh_l_ankle_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_ankle_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_ankle_pitch_link />
            </inertial>
        </link>
        <!-- left ankle roll pitch -->
        <joint
          name="l_ankle_roll_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="${pi/2} 0 0" />
          <parent
            link="l_ankle_pitch_link" />
          <child
            link="l_ankle_roll_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-0.3491"
            upper="0.3491"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="10" />
        </joint>
        <xacro:insert_transmission name="l_ankle_roll_joint"/>
        <!-- left ankle roll link -->
        <link name="l_ankle_roll_link">
            <visual>
                <xacro:insert_mesh_l_ankle_roll_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_ankle_roll_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_ankle_roll_link />
            </inertial>
        </link>

    </xacro:macro>
</robot>
