<?xml version="1.0"?>
<robot 
    xmlns:xacro="http://www.ros.org/wiki/xacro">
    <!-- left leg -->
    <xacro:macro name="huron_left_leg" params="parent dxl_effort dxl_speed">
        <!-- left hip yaw joint -->
        <joint name="l_hip_yaw_joint" type="revolute">
          <origin xyz="-0.0775 0 -0.14512" rpy="0 0 -0.0" />
          <parent link="${parent}" />
          <child link="l_hip_yaw_link" />
          <axis xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="5" />
        </joint>
        <xacro:insert_transmission name="l_hip_yaw_joint"/>
        <!-- left hip yaw link -->
        <link name="l_hip_yaw_link">
            <visual>
                <xacro:insert_mesh_l_hip_yaw_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_hip_yaw_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_hip_yaw_link />
            </inertial>
        </link>
        <!-- left hip roll joint-->
        <joint
          name="l_hip_roll_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="${pi/2} 0 ${pi}" />
          <parent
            link="l_hip_yaw_link" />
          <child
            link="l_hip_roll_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="5" />
        </joint>
        <xacro:insert_transmission name="l_hip_roll_joint"/>
        <!-- left hip roll link -->
        <link name="l_hip_roll_link">
            <visual>
                <xacro:insert_mesh_l_hip_roll_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_hip_roll_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_hip_roll_link />
            </inertial>
        </link>
        <!-- left hip pitch joint -->
        <joint
          name="l_hip_pitch_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="${-pi/2} 0 ${-pi/2}" />
          <parent
            link="l_hip_roll_link" />
          <child
            link="l_hip_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="5" />
        </joint>
        <xacro:insert_transmission name="l_hip_pitch_joint"/>
        <!-- left hip pitch link -->
        <link name="l_hip_pitch_link">
            <visual>
                <xacro:insert_mesh_l_hip_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_hip_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_hip_pitch_link />
            </inertial>
        </link>
        <!-- left knee pitch joint -->
        <joint
          name="l_knee_pitch_joint"
          type="revolute">
          <origin
            xyz="0.512778478308813 4.99999997527478E-05 1.66647320021251E-05"
            rpy="0 0 0" />
          <parent
            link="l_hip_pitch_link" />
          <child
            link="l_knee_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="5" />
        </joint>
        <xacro:insert_transmission name="l_knee_pitch_joint"/>
        <!-- left knee pitch link -->
        <link name="l_knee_pitch_link">
            <visual>
                <xacro:insert_mesh_l_knee_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_knee_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_knee_pitch_link />
            </inertial>
        </link>
        <!-- left ankle pitch joint -->
        <joint
          name="l_ankle_pitch_joint"
          type="revolute">
          <origin
            xyz="0.370000000000016 3.16358752296964E-05 0.00249999999999911"
            rpy="0 0 0.0" />
          <parent
            link="l_knee_pitch_link" />
          <child
            link="l_ankle_pitch_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="0.9" />
        </joint>
        <xacro:insert_transmission name="l_ankle_pitch_joint"/>
        <!-- left ankle pitch link -->
        <link name="l_ankle_pitch_link">
            <visual>
                <xacro:insert_mesh_l_ankle_pitch_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_ankle_pitch_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_ankle_pitch_link />
            </inertial>
        </link>
        <!-- left ankle roll pitch -->
        <joint
          name="l_ankle_roll_joint"
          type="revolute">
          <origin
            xyz="0 0 0"
            rpy="${pi/2} 0 0" />
          <parent
            link="l_ankle_pitch_link" />
          <child
            link="l_ankle_roll_link" />
          <axis
            xyz="0 0 1" />
          <limit
            lower="-3.1416"
            upper="3.1416"
            effort="${dxl_effort}"
            velocity="${dxl_speed}" />
          <dynamics
            damping="0.5"
            friction="0.9" />
        </joint>
        <xacro:insert_transmission name="l_ankle_roll_joint"/>
        <!-- left ankle roll link -->
        <link name="l_ankle_roll_link">
            <visual>
                <xacro:insert_mesh_l_ankle_roll_link />
            </visual>
            <collision>
                <xacro:insert_mesh_l_ankle_roll_link />
            </collision>
            <inertial>
                <xacro:insert_inertia_l_ankle_roll_link />
            </inertial>
        </link>

        <!-- left ft fixed joint 1 -->
        <joint name="l1_ft_fixed" type="revolute">
            <origin
              xyz="0.096 0.020682 0.13788"
              rpy="-0.0 0.0 ${pi/2}" />
            <axis xyz="0 0 1"/>
            <parent link="l_ankle_roll_link"/>
            <child link="l1_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- left foot ft link 1 -->
        <link name="l1_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_l1_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_l1_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_l1_foot_ft_link />
	    </inertial>
        </link>
        

        <!-- left ft fixed joint 2 -->
        <joint name="l2_ft_fixed" type="revolute">
            <origin
              xyz="0.096 -0.014318 0.13788"
              rpy="-0.0 0.0 ${pi/2}" />
            <axis xyz="0 0 1"/>
            <parent link="l_ankle_roll_link"/>
            <child link="l2_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- left foot ft link 2 -->
        <link name="l2_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_l2_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_l2_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_l2_foot_ft_link />
	    </inertial>
        </link>



        <!-- left ft fixed joint 3 -->
        <joint name="l3_ft_fixed" type="revolute">
            <origin
              xyz="0.096 -0.016649 -0.026"
              rpy="-0.0 0.0 ${pi/2}" />
            <axis xyz="0 0 1"/>
            <parent link="l_ankle_roll_link"/>
            <child link="l3_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- left foot ft link 3 -->
        <link name="l3_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_l3_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_l3_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_l3_foot_ft_link />
	    </inertial>
        </link>



        <!-- left ft fixed joint 4 -->
        <joint name="l4_ft_fixed" type="revolute">
            <origin
              xyz="0.096 0.01686 -0.026"
              rpy="-0.0 0.0 ${pi/2}" />
            <axis xyz="0 0 1"/>
            <parent link="l_ankle_roll_link"/>
            <child link="l4_foot_ft_link"/>
            <limit
            lower="0"
            upper="0"
            effort="100"
            velocity="100" />
        </joint>
        <!-- left foot ft link 4 -->
        <link name="l4_foot_ft_link">
	    <visual>
		<xacro:insert_mesh_l4_foot_ft_link />
	    </visual>
            <collision>
                <xacro:insert_mesh_l4_foot_ft_link />
            </collision>	    
	    <inertial>
		<xacro:insert_inertia_l4_foot_ft_link />
	    </inertial>
        </link>
         
        
        <!-- left ft fixed joint -->
        <joint name="lv_ft_fixed" type="fixed">
            <origin xyz="0.045 0.0 0.0" rpy="0 ${-pi/2} 0"/>
            <parent link="l4_foot_ft_link"/>
            <child link="l_foot_v_ft_link"/>
        </joint>
        <!-- left foot ft link -->
        <link name="l_foot_v_ft_link"></link>

    </xacro:macro>
</robot>
